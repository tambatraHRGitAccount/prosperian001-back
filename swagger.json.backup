      "post": { 
        "summary": "Create a new list with file upload", 
        "tags": ["List"], 
        "requestBody": { 
          "required": true, 
          "content": { 
            "multipart/form-data": { 
              "schema": { 
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string",
                    "description": "Type of list (Entreprise or Contact)",
                    "example": "Entreprise"
                  },
                  "nom": {
                    "type": "string",
                    "description": "Name of the list",
                    "example": "Ma liste d'entreprises"
                  },
                  "file": {
                    "type": "string",
                    "format": "binary",
                    "description": "CSV file to upload"
                  }
                },
                "required": ["type", "nom", "file"]
              }
            } 
          } 
        }, 
        "responses": { 
          "201": { 
            "description": "List created successfully", 
            "content": { 
              "application/json": { 
                "schema": { "$ref": "#/components/schemas/List" } 
              } 
            } 
